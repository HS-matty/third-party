<?xml version="1.0"?>
<ui_element name="login" type="form">

			<title>
				<en>Reset Password</en>
				<ru>Reset Password</ru>
			</title>

			<fields>
				
				<field name="email" type="input" is_required="1">
				
				<title>
						<en>Email</en>
						<ru>Емаил</ru>
						
					</title>
				
									
					<value type="string" class="email">
						<range min="10" max="30"></range>
					</value>
				
					<view is_allowed="0"></view>
					<edit is_allowed="0"></edit>
			
				</field>
		
			
		</fields>
	
		
	
</ui_element>