<?xml version="1.0"?>
<ui_element name="register" type="form">

			<title>
				<en>Change password</en>
				<ru>Изменить пароль</ru>
			</title>

			<fields>
				
				
				
			
			<field name="password" type="input" is_required="1">
				<title>
					<en>Password</en>
					<ru>Пароль</ru>
				</title>
				
				
					<value type="string" class="password">
						<range min="6" max="30"></range>
					</value>
				
				
				<view is_allowed="0" type="password">
					
				</view>
				<edit is_allowed="0"></edit>
			</field>
			
			<field name="password_retype" type="input" is_required="1">
				<title>
					<en>Retype password</en>
					<ru>Повтор пароля</ru>
				</title>
				
				
					<value type="string" class="password">
						<range min="6" max="30"></range>
					</value>
				
				
				<view is_allowed="0" type="password">
					
				</view>
				<edit is_allowed="0"></edit>
			</field>
			
			
		
