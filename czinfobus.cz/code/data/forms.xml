	<login_form>

		<field>
				<id>login</id> 
				<type>string</type>
				<length>
					<min>3</min>
					<max>15</max>
					<error_message>
						<en>Login length must be 3-15 characters</en>
						<ru>Длина имени пользователя должна быть от 3 до 15 знаков  </ru>
					</error_message>
				</length>
			
				<content>
					<expr>/^([A-Za-z0-9_])*$/</expr>
					<error_message>
						<en>Login must consist of characters and letters. </en>
						<ru>Имя пользователя должно состоять из букв латинского алфавита и цифр.</ru>
					</error_message>
				</content>							
		
		</field>			
		
