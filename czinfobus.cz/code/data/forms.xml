				<id>password</id>
				<type>string</type>
				<length>
					<min>4</min>
					<max>15</max>
					<error_message>
						<en>Password length must be 4-15 characters</en>
						<ru>Длина пароля должна быть от 4 до 15 знаков  </ru>
					</error_message>
				</length>
			
				<content>
					<expr>/^([A-Za-z0-9_.,-])*$/</expr>
					<error_message>
						<en>password must consist of characters, letters, [_.,-] sybmols</en>
						<ru>Пароль должен состоять из букв латинского алфавита, цифр, символов [ _.,- ]</ru>
					</error_message>
				</content>							
		
		</field>			
		
		
